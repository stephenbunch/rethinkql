{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "preserveConstEnums": true,
    "rootDir": "src",
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  },
  "include": [
    "src/**/*"
  ]
}
